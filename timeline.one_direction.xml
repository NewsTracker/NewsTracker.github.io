    <dc:date>2015-03-27T08:31:09Z</dc:date>
    <media:content url="http://cdn2.belfasttelegraph.co.uk/entertainment/news/article31098987.ece/428b0/ALTERNATES/h342/PANews%20BT_P-a80b4a47-aec9-4e3d-ace8-7502f268d4b6_I1.jpg" />
